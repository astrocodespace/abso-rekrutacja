{# {{ form_start(form) }} #}
<form method="post" enctype="multipart/form-data">
    <div class="form-group bmd-form-group">
        {{ form_row(form.name) }}
{#        <div class="text-danger">#}
{#            {{ form_errors(form.name) }}#}
{#        </div>#}
    </div>
    <div class="form-group bmd-form-group">
        <label for="logo">Logo</label>
        <div class="form-control">

            <input type="file" name="logo" style="position: unset; opacity: 1" />
        </div>
        <div class="text-small">
            Add image in format: png, jpg, bmp or gif
        </div>
{#        <div class="text-danger">#}
{#            {{ form_errors(form.logo) }}#}
{#        </div>#}
    </div>
    <div class="form-group bmd-form-group">
        {{ form_row(form.regon) }}
        <div class="text-small">
            REGON number should contain 9 or 14 numbers.
        </div>
{#        <div class="text-danger">#}
{#            {{ form_errors(form.regon) }}#}
{#        </div>#}
    </div>
    <div class="form-group bmd-form-group">
        {{ form_row(form.nip) }}
        <div class="text-small">
            NIP number should contain 10 numbers.
        </div>
{#        <div class="text-danger">#}
{#            {{ form_errors(form.nip) }}#}
{#        </div>#}
    </div>
    <div class="form-group bmd-form-group">
        {{ form_row(form.postalCode) }}
        <div class="text-small">
            Postal code should be in format 00-000
        </div>
{#        <div class="text-danger">#}
{#            {{ form_errors(form.postalCode) }}#}
{#        </div>#}

    </div>
    <div class="form-group bmd-form-group">
        {{ form_row(form.city) }}
{#        <div class="text-danger">#}
{#            {{ form_errors(form.city) }}#}
{#        </div>#}
    </div>
    <div class="form-group bmd-form-group">
        {{ form_row(form.street) }}
{#        <div class="text-danger">#}
{#            {{ form_errors(form.street) }}#}
{#        </div>#}
    </div>
    {{ form_row(form._token) }}

    <button class="btn">{{ button_label|default('Zapisz') }}</button>
    {# {{ form_end(form) }} #}
</form>
